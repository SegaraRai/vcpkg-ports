{
  "name": "vcpkg-ports",
  "version": "1.0.0",
  "author": "SegaraRai",
  "type": "module",
  "license": "MIT",
  "private": true,
  "scripts": {
    "astro": "astro",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "lint": "pnpm run lint:js && pnpm run lint:style",
    "lint:js": "eslint . && prettier -c .",
    "lint:style": "stylelint **/*.{css,scss} --ignore-path .gitignore",
    "lint:fix": "eslint . --fix && prettier -w . && pnpm run lint:style -- --fix",
    "generate": "pnpm run generate:data && pnpm run generate:images",
    "generate:data": "tsx scripts/bin/generateDataPorts.mts && tsx scripts/bin/generateDataHistory.mts",
    "generate:fonts": "tsx scripts/bin/generateFonts.mts",
    "generate:images": "tsx scripts/bin/generateImages.mts",
    "validate:manifests": "tsx scripts/bin/validatePortManifests.mts",
    "validate:portfiles": "tsx scripts/bin/parsePortfiles.mts",
    "validate:sync": "tsx scripts/bin/checkDataSync.mts",
    "sync:deps": "taze -w major && pnpm i && pnpm up",
    "sync:vcpkg": "cd .vcpkg && git checkout master && git pull --ff-only --depth=200 origin master && cd .. && pnpm run validate:manifests && pnpm run validate:portfiles && pnpm run generate:data",
    "sync:vcpkg-full": "cd .vcpkg && git checkout master && git reset --hard && git pull --ff-only --unshallow origin master && cd .. && pnpm run validate:manifests && pnpm run validate:portfiles && pnpm run generate:data --no-incremental",
    "typecheck": "astro check && vue-tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/vue": "^5.0.5",
    "@babel/core": "^7.26.0",
    "@eslint/js": "^9.18.0",
    "@iconify/json": "^2.2.295",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__js": "^8.42.3",
    "@types/fontkit": "^2.0.7",
    "@types/linkify-it": "^5.0.0",
    "@types/node": "^22.10.7",
    "@unocss/reset": "^65.4.2",
    "@vueuse/components": "^12.4.0",
    "@vueuse/core": "^12.4.0",
    "astro": "^5.1.7",
    "astro-compress": "^2.3.6",
    "beasties": "^0.2.0",
    "esbuild": "^0.24.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-vue": "^9.32.0",
    "fast-glob": "^3.3.3",
    "fontkit": "^2.0.4",
    "fuse.js": "^7.0.0",
    "html-entities": "^2.5.2",
    "jsep": "^1.4.0",
    "linkify-it": "^5.0.0",
    "postcss": "^8.5.1",
    "postcss-html": "^1.8.0",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-organize-imports": "^4.1.0",
    "rollup": "^4.30.1",
    "sass": "^1.83.4",
    "schema-dts": "^1.1.2",
    "sharp": "^0.33.5",
    "shiki": "^1.27.2",
    "spdx-license-list": "^6.9.0",
    "stylelint": "^16.13.2",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "subset-font": "^2.4.0",
    "taze": "^18.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "unocss": "^65.4.2",
    "unplugin-icons": "^22.0.0",
    "vite": "^6.0.7",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.0",
    "wrangler": "^3.103.0",
    "zod": "^3.24.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "sharp",
      "workerd"
    ],
    "patchedDependencies": {
      "beasties": "patches/beasties.patch"
    }
  }
}
