{
  "name": "vcpkg-ports",
  "version": "1.0.0",
  "author": "SegaraRai",
  "type": "module",
  "license": "MIT",
  "private": true,
  "scripts": {
    "astro": "astro",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "lint": "pnpm run lint:js && pnpm run lint:style",
    "lint:js": "eslint --ext .ts,.tsx,.cts,.mts,.js,.jsx,.cjs,.mjs . && prettier --check \"./**/*.{ts,tsx,cts,mts,js,jsx,cjs,mjs}\"",
    "lint:style": "stylelint **/*.{css,scss} --ignore-path .gitignore",
    "lint:fix": "eslint --ext .ts,.tsx,.cts,.mts,.js,.jsx,.cjs,.mjs . --fix && prettier --write \"./**/*.{ts,tsx,cts,mts,js,jsx,cjs,mjs}\" && pnpm run lint:style -- --fix",
    "generate": "pnpm run generate:data && pnpm run generate:images",
    "generate:data": "tsx scripts/bin/generateDataPorts.mts && tsx scripts/bin/generateDataHistory.mts",
    "generate:fonts": "tsx scripts/bin/generateFonts.mts",
    "generate:images": "tsx scripts/bin/generateImages.mts",
    "validate:manifests": "tsx scripts/bin/validatePortManifests.mts",
    "validate:portfiles": "tsx scripts/bin/parsePortfiles.mts",
    "validate:sync": "tsx scripts/bin/checkDataSync.mts",
    "sync:deps": "taze -w major && pnpm i && pnpm update",
    "sync:vcpkg": "cd .vcpkg && git pull --ff-only --depth=40 && cd .. && pnpm run validate:manifests && pnpm run validate:portfiles && pnpm run generate:data",
    "typecheck": "astro check && vue-tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@astrojs/sitemap": "^1.0.0",
    "@astrojs/vue": "^1.2.2",
    "@babel/core": "^7.20.7",
    "@iconify/json": "^2.1.158",
    "@types/fontkit": "^2.0.1",
    "@types/linkify-it": "^3.0.2",
    "@types/node": "^18.11.18",
    "@types/sharp": "^0.31.0",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "@unocss/reset": "^0.48.0",
    "@vueuse/components": "^9.9.0",
    "@vueuse/core": "^9.9.0",
    "astro": "^1.8.0",
    "astro-compress": "^1.1.24",
    "critters": "^0.0.16",
    "esbuild": "^0.16.12",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.12.0",
    "eslint-plugin-astro": "^0.21.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-unicorn": "^45.0.2",
    "fast-glob": "^3.2.12",
    "fontkit": "^2.0.2",
    "fuse.js": "^6.6.2",
    "html-entities": "^2.3.3",
    "jsep": "^1.3.8",
    "linkify-it": "^4.0.1",
    "postcss": "^8.4.20",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.1",
    "prettier-plugin-astro": "^0.7.0",
    "rollup": "^3.9.0",
    "sass": "^1.57.1",
    "schema-dts": "^1.1.0",
    "sharp": "^0.31.3",
    "shiki": "^0.12.1",
    "spdx-license-list": "^6.6.0",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "subset-font": "^2.0.0",
    "taze": "^0.8.5",
    "tsx": "^3.12.1",
    "typescript": "^4.9.4",
    "unocss": "^0.48.0",
    "unplugin-icons": "^0.14.15",
    "vite": "^3.2.5",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.18",
    "wrangler": "^2.6.2",
    "zod": "^3.20.2"
  }
}
