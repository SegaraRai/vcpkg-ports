{
  "name": "vcpkg-ports",
  "version": "1.0.0",
  "author": "SegaraRai",
  "type": "module",
  "license": "MIT",
  "private": true,
  "scripts": {
    "astro": "astro",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "lint": "pnpm run lint:js && pnpm run lint:style",
    "lint:js": "eslint --ext .ts,.tsx,.cts,.mts,.js,.jsx,.cjs,.mjs . && prettier --check \"./**/*.{ts,tsx,cts,mts,js,jsx,cjs,mjs}\"",
    "lint:style": "stylelint **/*.{css,scss} --ignore-path .gitignore",
    "lint:fix": "eslint --ext .ts,.tsx,.cts,.mts,.js,.jsx,.cjs,.mjs . --fix && prettier --write \"./**/*.{ts,tsx,cts,mts,js,jsx,cjs,mjs}\" && pnpm run lint:style -- --fix",
    "generate": "pnpm run generate:data && pnpm run generate:images",
    "generate:data": "tsx scripts/bin/generateDataPorts.mts && tsx scripts/bin/generateDataHistory.mts",
    "generate:fonts": "tsx scripts/bin/generateFonts.mts",
    "generate:images": "tsx scripts/bin/generateImages.mts",
    "validate:manifests": "tsx scripts/bin/validatePortManifests.mts",
    "validate:portfiles": "tsx scripts/bin/parsePortfiles.mts",
    "validate:sync": "tsx scripts/bin/checkDataSync.mts",
    "sync:deps": "taze -w major && pnpm i && pnpm update && pnpm dedupe",
    "sync:vcpkg": "cd .vcpkg && git pull --ff-only --depth=200 && cd .. && pnpm run validate:manifests && pnpm run validate:portfiles && pnpm run generate:data",
    "typecheck": "astro check && vue-tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@astrojs/sitemap": "^1.3.3",
    "@astrojs/vue": "^2.2.1",
    "@babel/core": "^7.22.5",
    "@iconify/json": "^2.2.82",
    "@types/fontkit": "^2.0.3",
    "@types/linkify-it": "^3.0.2",
    "@types/node": "^18.16.18",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@unocss/reset": "^0.53.3",
    "@vueuse/components": "^10.2.0",
    "@vueuse/core": "^10.2.0",
    "astro": "^2.7.0",
    "astro-compress": "^1.1.47",
    "critters": "^0.0.19",
    "esbuild": "^0.18.6",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-define-config": "^1.21.0",
    "eslint-plugin-astro": "^0.27.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-unicorn": "^47.0.0",
    "fast-glob": "^3.2.12",
    "fontkit": "^2.0.2",
    "fuse.js": "^6.6.2",
    "html-entities": "^2.3.6",
    "jsep": "^1.3.8",
    "linkify-it": "^4.0.1",
    "postcss": "^8.4.24",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.8",
    "prettier-plugin-astro": "^0.10.0",
    "rollup": "^3.25.1",
    "sass": "^1.63.6",
    "schema-dts": "^1.1.2",
    "sharp": "^0.32.1",
    "shiki": "^0.14.2",
    "spdx-license-list": "^6.6.0",
    "stylelint": "^15.9.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "subset-font": "^2.1.0",
    "taze": "^0.10.2",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3",
    "unocss": "^0.53.3",
    "unplugin-icons": "^0.16.3",
    "vite": "^4.3.9",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.1",
    "wrangler": "^3.1.1",
    "zod": "^3.21.4"
  }
}
