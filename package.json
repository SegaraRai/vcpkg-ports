{
  "name": "vcpkg-ports",
  "version": "1.0.0",
  "author": "SegaraRai",
  "type": "module",
  "license": "MIT",
  "private": true,
  "scripts": {
    "astro": "astro",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "lint": "pnpm run lint:js && pnpm run lint:style",
    "lint:js": "eslint --ext .ts,.tsx,.cts,.mts,.js,.jsx,.cjs,.mjs . && prettier --check \"./**/*.{ts,tsx,cts,mts,js,jsx,cjs,mjs}\"",
    "lint:style": "stylelint **/*.{css,scss} --ignore-path .gitignore",
    "lint:fix": "eslint --ext .ts,.tsx,.cts,.mts,.js,.jsx,.cjs,.mjs . --fix && prettier --write \"./**/*.{ts,tsx,cts,mts,js,jsx,cjs,mjs}\" && pnpm run lint:style -- --fix",
    "generate": "pnpm run generate:data && pnpm run generate:images",
    "generate:data": "tsx scripts/bin/generateDataPorts.mts && tsx scripts/bin/generateDataHistory.mts",
    "generate:fonts": "tsx scripts/bin/generateFonts.mts",
    "generate:images": "tsx scripts/bin/generateImages.mts",
    "validate:manifests": "tsx scripts/bin/validatePortManifests.mts",
    "validate:portfiles": "tsx scripts/bin/parsePortfiles.mts",
    "validate:sync": "tsx scripts/bin/checkDataSync.mts",
    "sync:deps": "taze -w major && pnpm i && pnpm update",
    "sync:vcpkg": "cd .vcpkg && git pull --ff-only --depth=200 && cd .. && pnpm run validate:manifests && pnpm run validate:portfiles && pnpm run generate:data",
    "typecheck": "astro check && vue-tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@astrojs/sitemap": "^1.0.1",
    "@astrojs/vue": "^2.0.1",
    "@babel/core": "^7.20.12",
    "@iconify/json": "^2.2.23",
    "@types/fontkit": "^2.0.1",
    "@types/linkify-it": "^3.0.2",
    "@types/node": "^18.13.0",
    "@types/sharp": "^0.31.1",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@unocss/reset": "^0.49.6",
    "@vueuse/components": "^9.12.0",
    "@vueuse/core": "^9.12.0",
    "astro": "^2.0.13",
    "astro-compress": "^1.1.33",
    "critters": "^0.0.16",
    "esbuild": "^0.17.8",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-define-config": "^1.15.0",
    "eslint-plugin-astro": "^0.23.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-unicorn": "^45.0.2",
    "fast-glob": "^3.2.12",
    "fontkit": "^2.0.2",
    "fuse.js": "^6.6.2",
    "html-entities": "^2.3.3",
    "jsep": "^1.3.8",
    "linkify-it": "^4.0.1",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.4",
    "prettier-plugin-astro": "^0.8.0",
    "rollup": "^3.15.0",
    "sass": "^1.58.2",
    "schema-dts": "^1.1.0",
    "sharp": "^0.31.3",
    "shiki": "^0.14.1",
    "spdx-license-list": "^6.6.0",
    "stylelint": "^15.1.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^30.0.1",
    "stylelint-config-standard-scss": "^7.0.1",
    "subset-font": "^2.0.0",
    "taze": "^0.8.5",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5",
    "unocss": "^0.49.6",
    "unplugin-icons": "^0.15.3",
    "vite": "^4.1.1",
    "vue": "^3.2.47",
    "vue-tsc": "^1.1.2",
    "wrangler": "^2.10.0",
    "zod": "^3.20.6"
  }
}
