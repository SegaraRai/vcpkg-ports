{
  "name": "vcpkg-ports",
  "version": "1.0.0",
  "author": "SegaraRai",
  "type": "module",
  "license": "MIT",
  "private": true,
  "scripts": {
    "astro": "astro",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "lint": "pnpm run lint:js && pnpm run lint:style",
    "lint:js": "eslint . && prettier -c .",
    "lint:style": "stylelint **/*.{css,scss} --ignore-path .gitignore",
    "lint:fix": "eslint . --fix && prettier -w . && pnpm run lint:style -- --fix",
    "generate": "pnpm run generate:data && pnpm run generate:images",
    "generate:data": "tsx scripts/bin/generateDataPorts.mts && tsx scripts/bin/generateDataHistory.mts",
    "generate:fonts": "tsx scripts/bin/generateFonts.mts",
    "generate:images": "tsx scripts/bin/generateImages.mts",
    "validate:manifests": "tsx scripts/bin/validatePortManifests.mts",
    "validate:portfiles": "tsx scripts/bin/parsePortfiles.mts",
    "validate:sync": "tsx scripts/bin/checkDataSync.mts",
    "sync:deps": "taze -w major && pnpm i && pnpm up",
    "sync:vcpkg": "cd .vcpkg && git checkout master && git pull --ff-only --depth=200 origin master && cd .. && pnpm run validate:manifests && pnpm run validate:portfiles && pnpm run generate:data",
    "sync:vcpkg-full": "cd .vcpkg && git checkout master && git reset --hard && git pull --ff-only origin master && cd .. && pnpm run validate:manifests && pnpm run validate:portfiles && pnpm run generate:data --no-incremental",
    "typecheck": "astro check && vue-tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.2",
    "@astrojs/sitemap": "^3.1.6",
    "@astrojs/vue": "^4.5.0",
    "@babel/core": "^7.25.2",
    "@eslint/js": "^9.9.0",
    "@iconify/json": "^2.2.237",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__js": "^8.42.3",
    "@types/fontkit": "^2.0.7",
    "@types/linkify-it": "^5.0.0",
    "@types/node": "^20.14.15",
    "@unocss/reset": "^0.62.1",
    "@vueuse/components": "^10.11.1",
    "@vueuse/core": "^10.11.1",
    "astro": "^4.13.4",
    "astro-compress": "^2.3.1",
    "critters": "^0.0.24",
    "esbuild": "^0.23.0",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-vue": "^9.27.0",
    "fast-glob": "^3.3.2",
    "fontkit": "^2.0.4",
    "fuse.js": "^7.0.0",
    "html-entities": "^2.5.2",
    "jsep": "^1.3.9",
    "linkify-it": "^5.0.0",
    "postcss": "^8.4.41",
    "postcss-html": "^1.7.0",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-organize-imports": "^4.0.0",
    "rollup": "^4.20.0",
    "sass": "^1.77.8",
    "schema-dts": "^1.1.2",
    "sharp": "^0.33.4",
    "shiki": "^1.12.1",
    "spdx-license-list": "^6.9.0",
    "stylelint": "^16.8.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "subset-font": "^2.3.0",
    "taze": "^0.16.5",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.1.0",
    "unocss": "^0.62.1",
    "unplugin-icons": "^0.19.2",
    "vite": "^5.4.0",
    "vue": "^3.4.37",
    "vue-tsc": "^2.0.29",
    "wrangler": "^3.71.0",
    "zod": "^3.23.8"
  }
}
