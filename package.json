{
  "name": "vcpkg-ports",
  "version": "1.0.0",
  "author": "SegaraRai",
  "type": "module",
  "license": "MIT",
  "private": true,
  "scripts": {
    "astro": "astro",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "lint": "pnpm run lint:js && pnpm run lint:style",
    "lint:js": "eslint . && prettier -c .",
    "lint:style": "stylelint **/*.{css,scss} --ignore-path .gitignore",
    "lint:fix": "eslint . --fix && prettier -w . && pnpm run lint:style -- --fix",
    "generate": "pnpm run generate:data && pnpm run generate:images",
    "generate:data": "tsx scripts/bin/generateDataPorts.mts && tsx scripts/bin/generateDataHistory.mts",
    "generate:fonts": "tsx scripts/bin/generateFonts.mts",
    "generate:images": "tsx scripts/bin/generateImages.mts",
    "validate:manifests": "tsx scripts/bin/validatePortManifests.mts",
    "validate:portfiles": "tsx scripts/bin/parsePortfiles.mts",
    "validate:sync": "tsx scripts/bin/checkDataSync.mts",
    "sync:deps": "taze -w major && pnpm i && pnpm up",
    "sync:vcpkg": "cd .vcpkg && git checkout master && git pull --ff-only --depth=200 origin master && cd .. && pnpm run validate:manifests && pnpm run validate:portfiles && pnpm run generate:data",
    "sync:vcpkg-full": "cd .vcpkg && git checkout master && git reset --hard && git pull --ff-only origin master && cd .. && pnpm run validate:manifests && pnpm run validate:portfiles && pnpm run generate:data --no-incremental",
    "typecheck": "astro check && vue-tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@astrojs/check": "^0.4.1",
    "@astrojs/sitemap": "^3.0.4",
    "@astrojs/vue": "^4.0.8",
    "@babel/core": "^7.23.7",
    "@iconify/json": "^2.2.169",
    "@types/fontkit": "^2.0.6",
    "@types/linkify-it": "^3.0.5",
    "@types/node": "^20.11.2",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@unocss/reset": "^0.58.3",
    "@vueuse/components": "^10.7.2",
    "@vueuse/core": "^10.7.2",
    "astro": "^4.1.2",
    "astro-compress": "^2.2.8",
    "critters": "^0.0.20",
    "esbuild": "^0.19.11",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-astro": "^0.31.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-unicorn": "^50.0.1",
    "fast-glob": "^3.3.2",
    "fontkit": "^2.0.2",
    "fuse.js": "^7.0.0",
    "html-entities": "^2.4.0",
    "jsep": "^1.3.8",
    "linkify-it": "^5.0.0",
    "postcss": "^8.4.33",
    "postcss-html": "^1.6.0",
    "prettier": "^3.2.2",
    "prettier-plugin-astro": "^0.12.3",
    "rollup": "^4.9.5",
    "sass": "^1.69.7",
    "schema-dts": "^1.1.2",
    "sharp": "^0.33.2",
    "shiki": "^0.14.7",
    "spdx-license-list": "^6.8.0",
    "stylelint": "^16.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "subset-font": "^2.1.0",
    "taze": "^0.13.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "unocss": "^0.58.3",
    "unplugin-icons": "^0.18.2",
    "vite": "^5.0.11",
    "vue": "^3.4.14",
    "vue-tsc": "^1.8.27",
    "wrangler": "^3.22.4",
    "zod": "^3.22.4"
  }
}
